{
  "name": "agenttown",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "android-device": "expo run:android --device",
    "ios": "expo start --ios",
    "ios-device": "expo run:ios --device",
    "android_native": "expo run:android",
    "ios_native": "expo run:ios",
    "web": "expo start --web",
    "start:clean": "expo start --clear",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:ci": "jest --ci --runInBand --watchAll=false",
    "e2e:build:ios": "detox build -c ios.sim.release",
    "e2e:test:ios": "npm run e2e:build:ios && detox test -c ios.sim.release --cleanup",
    "e2e:build:android": "detox build -c android.attached.release",
    "e2e:test:android": "npm run e2e:build:android && detox test -c android.attached.release --cleanup",
    "doctor": "expo doctor",
    "build:web": "expo export --platform web",
    "check": "npm run typecheck && npm run lint && npm run test:ci",
    "fastlane:prepare:ios": "fastlane ios prepare --env fastlane",
    "fastlane:build:ios": "fastlane ios build_release --env fastlane",
    "fastlane:deploy:ios": "fastlane ios deploy_testflight --env fastlane"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.33",
    "expo-apple-authentication": "~8.0.8",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-speech": "~14.0.8",
    "expo-speech-recognition": "^3.1.0",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gifted-chat": "^2.8.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-sse": "^1.2.1",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "eas-cli": "^18.0.1",
    "eslint": "^9.39.2",
    "eslint-config-expo": "^10.0.0",
    "detox": "^20.42.3",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.17",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.9.4"
}
